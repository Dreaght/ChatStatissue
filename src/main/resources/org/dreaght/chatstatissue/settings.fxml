<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dreaght.chatstatissue.controller.SettingsController">
    <children>
        <TextField fx:id="textField" layoutX="62.0" prefHeight="50.0" prefWidth="420.0" promptText="Type here...">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
      <Button fx:id="reconnectButton" layoutX="482.0" mnemonicParsing="false" onAction="#onReconnectButtonPress" prefHeight="50.0" prefWidth="43.0" text="🔄" />
      <Button fx:id="screenshare" layoutX="62.0" layoutY="49.0" mnemonicParsing="false" onAction="#onScreenshareButton" prefHeight="25.0" prefWidth="463.0" text="Screenshare">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="voice" mnemonicParsing="false" onAction="#onVoiceButton" prefHeight="74.0" prefWidth="62.0" text="၊၊||၊" />
    </children>
</Pane>
